{
  "name": "heimrichhannot/contao-entity-filter-bundle",
  "type": "contao-bundle",
  "description": "This module offers an input type for selecting one or more entities by comfortably 'clicking together' sql-like conditions.",
  "license": "LGPL-3.0-or-later",
  "require": {
    "php": "^7.1||^8.0",
    "contao/core-bundle": "^4.4",
    "heimrichhannot/contao-utils-bundle": "^2.0",
    "heimrichhannot/contao-multi-column-editor-bundle": "^2.0",
    "heimrichhannot/contao-list_widget": "^2.0",
    "heimrichhannot/contao-field-value-copier-bundle": "^1.0"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.0",
    "friendsofphp/php-cs-fixer": "^2.2",
    "phpunit/phpunit": "^6.0",
    "satooshi/php-coveralls": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\EntityFilterBundle\\": "src/"
    },
    "classmap": [
      "src/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "HeimrichHannot\\EntityFilterBundle\\Test\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\EntityFilterBundle\\ContaoManager\\Plugin"
  }
}
